  gem "wdm" if Gem.win_platform?
  gem 'jekyll-feed'
  gem 'jekyll-gist'
  gem 'jekyll-sitemap'
  gem 'jekyll-seo-tag'
  gem "jekyll-polyglot"
  gem 'jemoji'
  gem 'jekyll-toc'
  gem 'jekyll-diagrams'
end
gem "webrick", "~> 1.8.1"
