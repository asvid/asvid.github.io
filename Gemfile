source 'https://rubygems.org'
gem 'jekyll'

group :jekyll_plugins do
  gem "jekyll-paginate"
  gem "pygments.rb"
  gem "wdm" if Gem.win_platform?
  gem 'jekyll-feed'
  gem 'jekyll-gist'
  gem 'jekyll-sitemap'
  gem 'jekyll-seo-tag'
  gem "jekyll-polyglot"
  gem 'jekyll-commonmark-ghpages'
  gem 'jemoji'
end